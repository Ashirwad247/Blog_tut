<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head.ejs') %>
    <title>Some thing</title>
    
</head>
<body>
    <%- include('./partials/nav.ejs') %>
    <div class="container mt-3">
        <div class="row row-cols-4">

            <% blogs.forEach(blog => { %>
                    <div class="card">
                        <img src="<%= blog.coverImageURL%>" alt="" class="card-img-top" alt="..." width="10px">
                        <div class="card-body">
                            <h5 class="card-title"><%= blog.title %></h5>
                            <h1><%= blog.createdBy.name %></h1>
                            <a href="/blog/<%= blog._id %>" class="btn btn-primary">View</a>
                        </div>
                    </div>
            <% }) %>
        
        </div>
    </div>



    <%- include('./partials/script.ejs') %>
</body>
</html>